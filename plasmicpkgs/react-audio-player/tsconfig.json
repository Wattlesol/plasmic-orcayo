{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react",
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@plasmicapp/host": ["../../packages/host/src"],
      "@plasmicapp/host/*": ["../../packages/host/src/*"]
    }
 },
  "include": [
    "src/**/*"
  ]
}