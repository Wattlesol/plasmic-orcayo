{
  "name": "@plasmicpkgs/plasmic-content-stack",
  "version": "0.0.182",
  "description": "Plasmic ContentStack components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plasmicapp/plasmic.git",
    "directory": "plasmicpkgs/plasmic-content-stack"
 },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
 "scripts": {
    "build": "yarn build:types && yarn build:index",
    "build:types": "yarn tsc",
    "build:index": "node ../../build.mjs ./src/index.tsx --use-client",
    "test": "TEST_CWD=`pwd` yarn --cwd=../.. test --passWithNoTests",
    "prepublishOnly": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@plasmicapp/host": "1.0.227",
    "@plasmicapp/query": "0.1.81",
    "@types/dlv": "^1.1.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "husky": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "contentstack": "^3.15.1",
    "dlv": "^1.1.3"
  }
}
